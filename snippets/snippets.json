{
    "doctype": {
      "prefix": "adoctype",
      "body": "<!DOCTYPE html>",
      "description": "HTML5 doctype declaration",
      "scope": "text.html"
    },
    "html5_boilerplate": {
      "prefix": "ahtml5",
      "body": [
        "<!DOCTYPE html>",
        "<html lang=\"${1:en}\">",
        "<head>",
        "\t<meta charset=\"UTF-8\">",
        "\t<meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">",
        "\t<meta http-equiv=\"X-UA-Compatible\" content=\"ie=edge\">",
        "\t<title>${2:Document}</title>",
        "\t<link rel=\"stylesheet\" href=\"${3:styles.css}\">",
        "</head>",
        "<body>",
        "\t${0}",
        "</body>",
        "</html>"
      ],
      "description": "Complete HTML5 boilerplate with responsive meta tags",
      "scope": "text.html"
    },
  
    "django_block": {
      "prefix": "adblock",
      "body": [
        "{% block ${1:content} %}",
        "\t${0}",
        "{% endblock %}"
      ],
      "description": "Django template block",
      "scope": "text.html.django"
    },
    "django_static": {
      "prefix": "adstatic",
      "body": "{% static '${1:path/to/file}' %}",
      "description": "Django static file reference",
      "scope": "text.html.django"
    },
    "django_forloop": {
      "prefix": "adfor",
      "body": [
        "{% for ${1:item} in ${2:list} %}",
        "\t${0}",
        "{% endfor %}"
      ],
      "description": "Django for loop template tag",
      "scope": "text.html.django"
    },
  
    "responsive_card": {
      "prefix": "acard",
      "body": [
        "<div class=\"card\">",
        "\t<img src=\"${1:image.jpg}\" alt=\"${2:alt-text}\" class=\"card-img\">",
        "\t<div class=\"card-body\">",
        "\t\t<h3 class=\"card-title\">${3:Title}</h3>",
        "\t\t<p class=\"card-text\">${4:Description}</p>",
        "\t\t<a href=\"${5:#}\" class=\"btn btn-primary\">${6:Button}</a>",
        "\t</div>",
        "</div>"
      ],
      "description": "Bootstrap-style responsive card component",
      "scope": "text.html"
    },
    "navbar": {
      "prefix": "anavbar",
      "body": [
        "<nav class=\"navbar navbar-expand-lg navbar-light bg-light\">",
        "\t<div class=\"container-fluid\">",
        "\t\t<a class=\"navbar-brand\" href=\"#\">${1:Logo}</a>",
        "\t\t<button class=\"navbar-toggler\" type=\"button\" data-bs-toggle=\"collapse\" data-bs-target=\"#navbarNav\">",
        "\t\t\t<span class=\"navbar-toggler-icon\"></span>",
        "\t\t</button>",
        "\t\t<div class=\"collapse navbar-collapse\" id=\"navbarNav\">",
        "\t\t\t<ul class=\"navbar-nav\">",
        "\t\t\t\t<li class=\"nav-item\">",
        "\t\t\t\t\t<a class=\"nav-link active\" href=\"#\">${2:Home}</a>",
        "\t\t\t\t</li>",
        "\t\t\t\t<li class=\"nav-item\">",
        "\t\t\t\t\t<a class=\"nav-link\" href=\"#\">${3:About}</a>",
        "\t\t\t\t</li>",
        "\t\t\t</ul>",
        "\t\t</div>",
        "\t</div>",
        "</nav>"
      ],
      "description": "Responsive Bootstrap 5 navbar",
      "scope": "text.html"
    },
    "django_form": {
      "prefix": "adform",
      "body": [
        "<form method=\"${1:post}\" action=\"${2:#}\">",
        "\t{% csrf_token %}",
        "\t{{ form.as_p }}",
        "\t<button type=\"submit\" class=\"btn btn-primary\">${3:Submit}</button>",
        "</form>"
      ],
      "description": "Django form with CSRF protection",
      "scope": "text.html.django"
    },
    "input_group": {
      "prefix": "ainputgroup",
      "body": [
        "<div class=\"mb-3\">",
        "\t<label for=\"${1:id}\" class=\"form-label\">${2:Label}</label>",
        "\t<input type=\"${3:text}\" class=\"form-control\" id=\"${1:id}\" name=\"${4:name}\" placeholder=\"${5:Placeholder}\">",
        "</div>"
      ],
      "description": "Bootstrap 5 input group with label",
      "scope": "text.html"
    },
  
    "responsive_image": {
      "prefix": "aimg_responsive",
      "body": "<img src=\"${1:image.jpg}\" alt=\"${2:alt-text}\" class=\"img-fluid\" loading=\"lazy\">",
      "description": "Responsive image with lazy loading",
      "scope": "text.html"
    },
    "video_embed": {
      "prefix": "avideo_embed",
      "body": [
        "<div class=\"ratio ratio-16x9\">",
        "\t<iframe src=\"${1:https://www.youtube.com/embed/...}\" allowfullscreen></iframe>",
        "</div>"
      ],
      "description": "Responsive video embed (Bootstrap 5)",
      "scope": "text.html"
    },
  
    "skip_link": {
      "prefix": "askip_link",
      "body": "<a href=\"#main\" class=\"visually-hidden-focusable\">Skip to main content</a>",
      "description": "Accessibility skip link",
      "scope": "text.html"
    },
    "aria_alert": {
      "prefix": "aaria_alert",
      "body": "<div role=\"alert\" aria-live=\"polite\">${0}</div>",
      "description": "Accessible alert notification",
      "scope": "text.html"
    },
  
    "font_awesome": {
      "prefix": "afa",
      "body": "<i class=\"${1:fas} fa-${2:icon}\"></i>",
      "description": "Font Awesome icon",
      "scope": "text.html"
    },
    "svg_icon": {
      "prefix": "asvg",
      "body": [
        "<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"${1:16}\" height=\"${1:16}\" fill=\"currentColor\" class=\"bi bi-${2:icon}\" viewBox=\"0 0 16 16\">",
        "\t<path d=\"${3}\"/>",
        "</svg>"
      ],
      "description": "Bootstrap Icons SVG snippet",
      "scope": "text.html"
    },
  
    "container": {
      "prefix": "acontainer",
      "body": [
        "<div class=\"container\">",
        "\t${0}",
        "</div>"
      ],
      "description": "Bootstrap container",
      "scope": "text.html"
    },
    "grid_row": {
        "prefix": "aarow",
        "body": [
        "<div class=\"row\">",
        "\t<div class=\"col\">${1}</div>",
        "\t<div class=\"col\">${2}</div>",
        "</div>"
        ],
        "description": "Bootstrap grid row with 2 columns",
        "scope": "text.html"
    },

    "django_if": {
        "prefix": "aaif",
        "body": [
            "{% if ${1:condition} %}",
            "\t${2}",
            "{% else %}",
            "\t${3}",
            "{% endif %}"
        ],
        "description": "Django if-else template tag",
        "scope": "text.html.django"
    },

    "django_include": {
        "prefix": "aainclude",
        "body": "{% include '${1:template_name.html}' %}",
        "description": "Django include template tag",
        "scope": "text.html.django"
    },

    "django_url": {
        "prefix": "aaurl",
        "body": "{% url '${1:url_name}' %}",
        "description": "Django URL template tag",
        "scope": "text.html.django"
    },

    "django_extends": {
        "prefix": "aaextends",
        "body": "{% extends '${1:base.html}' %}",
        "description": "Django extends template tag",
        "scope": "text.html.django"
    },

    "bootstrap_modal": {
        "prefix": "aamodal",
        "body": [
            "<div class=\"modal fade\" id=\"${1:modalId}\" tabindex=\"-1\" aria-labelledby=\"${2:modalLabel}\" aria-hidden=\"true\">",
            "\t<div class=\"modal-dialog\">",
            "\t\t<div class=\"modal-content\">",
            "\t\t\t<div class=\"modal-header\">",
            "\t\t\t\t<h5 class=\"modal-title\" id=\"${2:modalLabel}\">${3:Modal Title}</h5>",
            "\t\t\t\t<button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>",
            "\t\t\t</div>",
            "\t\t\t<div class=\"modal-body\">",
            "\t\t\t\t${4:Modal content goes here}",
            "\t\t\t</div>",
            "\t\t\t<div class=\"modal-footer\">",
            "\t\t\t\t<button type=\"button\" class=\"btn btn-secondary\" data-bs-dismiss=\"modal\">Close</button>",
            "\t\t\t\t<button type=\"button\" class=\"btn btn-primary\">${5:Save changes}</button>",
            "\t\t\t</div>",
            "\t\t</div>",
            "\t</div>",
            "</div>"
        ],
        "description": "Bootstrap 5 modal component",
        "scope": "text.html"
    },

    "django_crispy_form": {
        "prefix": "aacrispy",
        "body": [
            "{% load crispy_forms_tags %}",
            "<form method=\"${1:post}\" action=\"${2:#}\">",
            "\t{% csrf_token %}",
            "\t{{ form|crispy }}",
            "\t<button type=\"submit\" class=\"btn btn-primary\">${3:Submit}</button>",
            "</form>"
        ],
        "description": "Django form with crispy-forms",
        "scope": "text.html.django"
    },

    "bootstrap_alert": {
        "prefix": "aaalert",
        "body": [
            "<div class=\"alert alert-${1:primary} alert-dismissible fade show\" role=\"alert\">",
            "\t${2:Alert message}",
            "\t<button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"alert\" aria-label=\"Close\"></button>",
            "</div>"
        ],
        "description": "Bootstrap 5 dismissible alert",
        "scope": "text.html"
    },

    "django_messages": {
        "prefix": "aamessages",
        "body": [
            "{% if messages %}",
            "<div class=\"messages\">",
            "\t{% for message in messages %}",
            "\t<div class=\"alert alert-{{ message.tags }} alert-dismissible fade show\" role=\"alert\">",
            "\t\t{{ message }}",
            "\t\t<button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"alert\" aria-label=\"Close\"></button>",
            "\t</div>",
            "\t{% endfor %}",
            "</div>",
            "{% endif %}"
        ],
        "description": "Django messages framework with Bootstrap alerts",
        "scope": "text.html.django"
    },

    "bootstrap_tabs": {
        "prefix": "aatabs",
        "body": [
            "<ul class=\"nav nav-tabs\" id=\"${1:myTab}\" role=\"tablist\">",
            "\t<li class=\"nav-item\" role=\"presentation\">",
            "\t\t<button class=\"nav-link active\" id=\"${2:home}-tab\" data-bs-toggle=\"tab\" data-bs-target=\"#${2:home}\" type=\"button\" role=\"tab\" aria-controls=\"${2:home}\" aria-selected=\"true\">${3:Home}</button>",
            "\t</li>",
            "\t<li class=\"nav-item\" role=\"presentation\">",
            "\t\t<button class=\"nav-link\" id=\"${4:profile}-tab\" data-bs-toggle=\"tab\" data-bs-target=\"#${4:profile}\" type=\"button\" role=\"tab\" aria-controls=\"${4:profile}\" aria-selected=\"false\">${5:Profile}</button>",
            "\t</li>",
            "</ul>",
            "<div class=\"tab-content\" id=\"${1:myTab}Content\">",
            "\t<div class=\"tab-pane fade show active\" id=\"${2:home}\" role=\"tabpanel\" aria-labelledby=\"${2:home}-tab\">${6:Home content}</div>",
            "\t<div class=\"tab-pane fade\" id=\"${4:profile}\" role=\"tabpanel\" aria-labelledby=\"${4:profile}-tab\">${7:Profile content}</div>",
            "</div>"
        ],
        "description": "Bootstrap 5 tabs component",
        "scope": "text.html"
    },

    "html_link": {
        "prefix": "aalink",
        "body": "<a href=\"${1:#}\" class=\"${2:link}\">${3:Link text}</a>",
        "description": "HTML link with class",
        "scope": "text.html"
    },

    "html_image": {
        "prefix": "aaimg",
        "body": "<img src=\"${1:image.jpg}\" alt=\"${2:description}\" class=\"${3:img-fluid}\">",
        "description": "HTML image with class",
        "scope": "text.html"
    },

    "html_paragraph": {
        "prefix": "aap",
        "body": "<p class=\"${1:text}\">${2:content}</p>",
        "description": "HTML paragraph with class",
        "scope": "text.html"
    },

    "html_button": {
        "prefix": "aabtn",
        "body": "<button type=\"${1:button}\" class=\"btn btn-${2:primary}\">${3:Button text}</button>",
        "description": "HTML button with Bootstrap classes",
        "scope": "text.html"
    },

    "html_div": {
        "prefix": "aadiv",
        "body": [
            "<div class=\"${1:container}\">",
            "\t${2:content}",
            "</div>"
        ],
        "description": "HTML div with class",
        "scope": "text.html"
    },

    "html_section": {
        "prefix": "aasection",
        "body": [
            "<section class=\"${1:section}\">",
            "\t<h2>${2:Section Title}</h2>",
            "\t${3:content}",
            "</section>"
        ],
        "description": "HTML section with heading",
        "scope": "text.html"
    },

    "html_list_unordered": {
        "prefix": "aaul",
        "body": [
            "<ul class=\"${1:list}\">",
            "\t<li>${2:First item}</li>",
            "\t<li>${3:Second item}</li>",
            "\t<li>${4:Third item}</li>",
            "</ul>"
        ],
        "description": "HTML unordered list with items",
        "scope": "text.html"
    },

    "html_list_ordered": {
        "prefix": "aaol",
        "body": [
            "<ol class=\"${1:list}\">",
            "\t<li>${2:First item}</li>",
            "\t<li>${3:Second item}</li>",
            "\t<li>${4:Third item}</li>",
            "</ol>"
        ],
        "description": "HTML ordered list with items",
        "scope": "text.html"
    },

    "html_table": {
        "prefix": "aatable",
        "body": [
            "<table class=\"table\">",
            "\t<thead>",
            "\t\t<tr>",
            "\t\t\t<th>${1:Header 1}</th>",
            "\t\t\t<th>${2:Header 2}</th>",
            "\t\t\t<th>${3:Header 3}</th>",
            "\t\t</tr>",
            "\t</thead>",
            "\t<tbody>",
            "\t\t<tr>",
            "\t\t\t<td>${4:Data 1}</td>",
            "\t\t\t<td>${5:Data 2}</td>",
            "\t\t\t<td>${6:Data 3}</td>",
            "\t\t</tr>",
            "\t</tbody>",
            "</table>"
        ],
        "description": "HTML table with Bootstrap classes",
        "scope": "text.html"
    },

    "html_form_input": {
        "prefix": "aainput",
        "body": [
            "<div class=\"form-group\">",
            "\t<label for=\"${1:inputId}\">${2:Label Text}</label>",
            "\t<input type=\"${3:text}\" class=\"form-control\" id=\"${1:inputId}\" name=\"${4:inputName}\" placeholder=\"${5:Enter text}\">",
            "</div>"
        ],
        "description": "HTML form input with Bootstrap classes",
        "scope": "text.html"
    },

    "html_textarea": {
        "prefix": "aatextarea",
        "body": [
            "<div class=\"form-group\">",
            "\t<label for=\"${1:textareaId}\">${2:Label Text}</label>",
            "\t<textarea class=\"form-control\" id=\"${1:textareaId}\" name=\"${3:textareaName}\" rows=\"${4:3}\">${5:}</textarea>",
            "</div>"
        ],
        "description": "HTML textarea with Bootstrap classes",
        "scope": "text.html"
    },

    "html_select": {
        "prefix": "aaselect",
        "body": [
            "<div class=\"form-group\">",
            "\t<label for=\"${1:selectId}\">${2:Label Text}</label>",
            "\t<select class=\"form-control\" id=\"${1:selectId}\" name=\"${3:selectName}\">",
            "\t\t<option value=\"${4:value1}\">${5:Option 1}</option>",
            "\t\t<option value=\"${6:value2}\">${7:Option 2}</option>",
            "\t\t<option value=\"${8:value3}\">${9:Option 3}</option>",
            "\t</select>",
            "</div>"
        ],
        "description": "HTML select with Bootstrap classes",
        "scope": "text.html"
    },

    "html_checkbox": {
        "prefix": "aacheckbox",
        "body": [
            "<div class=\"form-check\">",
            "\t<input type=\"checkbox\" class=\"form-check-input\" id=\"${1:checkboxId}\" name=\"${2:checkboxName}\">",
            "\t<label class=\"form-check-label\" for=\"${1:checkboxId}\">${3:Checkbox label}</label>",
            "</div>"
        ],
        "description": "HTML checkbox with Bootstrap classes",
        "scope": "text.html"
    },

    "html_radio": {
        "prefix": "aaradio",
        "body": [
            "<div class=\"form-check\">",
            "\t<input type=\"radio\" class=\"form-check-input\" id=\"${1:radioId}\" name=\"${2:radioName}\">",
            "\t<label class=\"form-check-label\" for=\"${1:radioId}\">${3:Radio label}</label>",
            "</div>"
        ],
        "description": "HTML radio with Bootstrap classes",
        "scope": "text.html"
    },

    "html_header": {
        "prefix": "aaheader",
        "body": [
            "<header class=\"${1:header}\">",
            "\t<h1>${2:Page Title}</h1>",
            "\t${3:content}",
            "</header>"
        ],
        "description": "HTML header with content",
        "scope": "text.html"
    },

    "html_footer": {
        "prefix": "aafooter",
        "body": [
            "<footer class=\"${1:footer}\">",
            "\t<p>&copy; ${2:2024} ${3:Your Company}. All rights reserved.</p>",
            "\t${4:content}",
            "</footer>"
        ],
        "description": "HTML footer with copyright",
        "scope": "text.html"
    },

    "django_csrf": {
        "prefix": "aacsrf",
        "body": "{% csrf_token %}",
        "description": "Django CSRF token tag",
        "scope": "text.html.django"
    },

    "django_load": {
        "prefix": "aaload",
        "body": "{% load ${1:static} %}",
        "description": "Django load template tag",
        "scope": "text.html.django"
    },

    "django_comment": {
        "prefix": "aacomment",
        "body": "{% comment %}${1:comment text}{% endcomment %}",
        "description": "Django comment tag",
        "scope": "text.html.django"
    },

    "django_with": {
        "prefix": "aawith",
        "body": [
            "{% with ${1:variable}=${2:value} %}",
            "\t${3:content}",
            "{% endwith %}"
        ],
        "description": "Django with template tag",
        "scope": "text.html.django"
    },

    "django_filter": {
        "prefix": "aafilter",
        "body": "{{ ${1:variable}|${2:filter}:${3:parameter} }}",
        "description": "Django filter syntax",
        "scope": "text.html.django"
    },

    "django_include_with": {
        "prefix": "aaincludewith",
        "body": "{% include '${1:template.html}' with ${2:variable}=${3:value} %}",
        "description": "Django include with parameters",
        "scope": "text.html.django"
    },

    "bootstrap_card_full": {
        "prefix": "aacardplus",
        "body": [
            "<div class=\"card shadow-sm\">",
            "\t<div class=\"card-header bg-${1:primary} text-white\">",
            "\t\t<h5 class=\"card-title mb-0\">${2:Card Title}</h5>",
            "\t</div>",
            "\t<div class=\"card-body\">",
            "\t\t<h6 class=\"card-subtitle mb-2 text-muted\">${3:Card Subtitle}</h6>",
            "\t\t<p class=\"card-text\">${4:Card content goes here}</p>",
            "\t\t<a href=\"${5:#}\" class=\"btn btn-${1:primary}\">${6:Learn More}</a>",
            "\t</div>",
            "\t<div class=\"card-footer text-muted\">",
            "\t\t${7:Last updated 3 mins ago}",
            "\t</div>",
            "</div>"
        ],
        "description": "Bootstrap 5 card with header, body, and footer",
        "scope": "text.html"
    },

    "bootstrap_grid_responsive": {
        "prefix": "aagrid",
        "body": [
            "<div class=\"container\">",
            "\t<div class=\"row\">",
            "\t\t<div class=\"col-12 col-md-6 col-lg-4\">",
            "\t\t\t${1:Column 1 content}",
            "\t\t</div>",
            "\t\t<div class=\"col-12 col-md-6 col-lg-4\">",
            "\t\t\t${2:Column 2 content}",
            "\t\t</div>",
            "\t\t<div class=\"col-12 col-md-6 col-lg-4\">",
            "\t\t\t${3:Column 3 content}",
            "\t\t</div>",
            "\t</div>",
            "</div>"
        ],
        "description": "Bootstrap 5 responsive grid system",
        "scope": "text.html"
    },

    "bootstrap_navbar_search": {
        "prefix": "aanavbarsearch",
        "body": [
            "<nav class=\"navbar navbar-expand-lg navbar-dark bg-${1:primary}\">",
            "\t<div class=\"container-fluid\">",
            "\t\t<a class=\"navbar-brand\" href=\"#\">${2:Brand}</a>",
            "\t\t<button class=\"navbar-toggler\" type=\"button\" data-bs-toggle=\"collapse\" data-bs-target=\"#navbarSearch\" aria-controls=\"navbarSearch\" aria-expanded=\"false\" aria-label=\"Toggle navigation\">",
            "\t\t\t<span class=\"navbar-toggler-icon\"></span>",
            "\t\t</button>",
            "\t\t<div class=\"collapse navbar-collapse\" id=\"navbarSearch\">",
            "\t\t\t<ul class=\"navbar-nav me-auto mb-2 mb-lg-0\">",
            "\t\t\t\t<li class=\"nav-item\">",
            "\t\t\t\t\t<a class=\"nav-link active\" href=\"#\">Home</a>",
            "\t\t\t\t</li>",
            "\t\t\t</ul>",
            "\t\t\t<form class=\"d-flex\">",
            "\t\t\t\t<input class=\"form-control me-2\" type=\"search\" placeholder=\"Search\" aria-label=\"Search\">",
            "\t\t\t\t<button class=\"btn btn-outline-light\" type=\"submit\">Search</button>",
            "\t\t\t</form>",
            "\t\t</div>",
            "\t</div>",
            "</nav>"
        ],
        "description": "Bootstrap 5 navbar with search form",
        "scope": "text.html"
    },

    "bootstrap_pagination": {
        "prefix": "aapagination",
        "body": [
            "<nav aria-label=\"Page navigation\">",
            "\t<ul class=\"pagination justify-content-center\">",
            "\t\t<li class=\"page-item disabled\">",
            "\t\t\t<a class=\"page-link\" href=\"#\" tabindex=\"-1\" aria-disabled=\"true\">Previous</a>",
            "\t\t</li>",
            "\t\t<li class=\"page-item active\" aria-current=\"page\">",
            "\t\t\t<a class=\"page-link\" href=\"#\">1</a>",
            "\t\t</li>",
            "\t\t<li class=\"page-item\"><a class=\"page-link\" href=\"#\">2</a></li>",
            "\t\t<li class=\"page-item\"><a class=\"page-link\" href=\"#\">3</a></li>",
            "\t\t<li class=\"page-item\">",
            "\t\t\t<a class=\"page-link\" href=\"#\">Next</a>",
            "\t\t</li>",
            "\t</ul>",
            "</nav>"
        ],
        "description": "Bootstrap 5 pagination component",
        "scope": "text.html"
    },

    "django_pagination": {
        "prefix": "aadpagination",
        "body": [
            "{% if page_obj.has_other_pages %}",
            "<nav aria-label=\"Page navigation\">",
            "\t<ul class=\"pagination justify-content-center\">",
            "\t\t{% if page_obj.has_previous %}",
            "\t\t<li class=\"page-item\">",
            "\t\t\t<a class=\"page-link\" href=\"?page={{ page_obj.previous_page_number }}\">Previous</a>",
            "\t\t</li>",
            "\t\t{% else %}",
            "\t\t<li class=\"page-item disabled\">",
            "\t\t\t<span class=\"page-link\">Previous</span>",
            "\t\t</li>",
            "\t\t{% endif %}",
            "\t\t",
            "\t\t{% for num in page_obj.paginator.page_range %}",
            "\t\t\t{% if page_obj.number == num %}",
            "\t\t\t<li class=\"page-item active\"><span class=\"page-link\">{{ num }}</span></li>",
            "\t\t\t{% else %}",
            "\t\t\t<li class=\"page-item\"><a class=\"page-link\" href=\"?page={{ num }}\">{{ num }}</a></li>",
            "\t\t\t{% endif %}",
            "\t\t{% endfor %}",
            "\t\t",
            "\t\t{% if page_obj.has_next %}",
            "\t\t<li class=\"page-item\">",
            "\t\t\t<a class=\"page-link\" href=\"?page={{ page_obj.next_page_number }}\">Next</a>",
            "\t\t</li>",
            "\t\t{% else %}",
            "\t\t<li class=\"page-item disabled\">",
            "\t\t\t<span class=\"page-link\">Next</span>",
            "\t\t</li>",
            "\t\t{% endif %}",
            "\t</ul>",
            "</nav>",
            "{% endif %}"
        ],
        "description": "Django pagination with Bootstrap 5",
        "scope": "text.html.django"
    },

    "bootstrap_toast": {
        "prefix": "aatoast",
        "body": [
            "<div class=\"toast\" role=\"alert\" aria-live=\"polite\" aria-atomic=\"true\">",
            "\t<div class=\"toast-header\">",
            "\t\t<strong class=\"me-auto\">${1:Title}</strong>",
            "\t\t<small>${2:11 mins ago}</small>",
            "\t\t<button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"toast\" aria-label=\"Close\"></button>",
            "\t</div>",
            "\t<div class=\"toast-body\">",
            "\t\t${3:Toast message goes here}",
            "\t</div>",
            "</div>"
        ],
        "description": "Bootstrap 5 toast component",
        "scope": "text.html"
    },

    "meta_tags_seo": {
        "prefix": "aaseo",
        "body": [
            "<meta name=\"description\" content=\"${1:Page description}\">",
            "<meta name=\"keywords\" content=\"${2:keywords,separated,by,commas}\">",
            "<meta name=\"author\" content=\"${3:Author name}\">",
            "<meta property=\"og:title\" content=\"${4:Title}\">",
            "<meta property=\"og:description\" content=\"${1:Page description}\">",
            "<meta property=\"og:image\" content=\"${5:image-url}\">",
            "<meta property=\"og:url\" content=\"${6:page-url}\">",
            "<meta name=\"twitter:card\" content=\"summary_large_image\">"
        ],
        "description": "Common SEO meta tags",
        "scope": "text.html"
    },

    "responsive_hero": {
        "prefix": "aahero",
        "body": [
            "<div class=\"px-4 py-5 my-5 text-center\">",
            "\t<img class=\"d-block mx-auto mb-4\" src=\"${1:logo.svg}\" alt=\"${2:Logo}\" width=\"${3:72}\" height=\"${4:57}\">",
            "\t<h1 class=\"display-5 fw-bold\">${5:Hero Title}</h1>",
            "\t<div class=\"col-lg-6 mx-auto\">",
            "\t\t<p class=\"lead mb-4\">${6:Hero description text goes here}</p>",
            "\t\t<div class=\"d-grid gap-2 d-sm-flex justify-content-sm-center\">",
            "\t\t\t<button type=\"button\" class=\"btn btn-primary btn-lg px-4 gap-3\">${7:Primary Action}</button>",
            "\t\t\t<button type=\"button\" class=\"btn btn-outline-secondary btn-lg px-4\">${8:Secondary Action}</button>",
            "\t\t</div>",
            "\t</div>",
            "</div>"
        ],
        "description": "Bootstrap 5 hero section",
        "scope": "text.html"
    }
}