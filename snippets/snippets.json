{
    "doctype": {
      "prefix": "adoctype",
      "body": "<!DOCTYPE html>",
      "description": "HTML5 doctype declaration",
      "scope": "text.html"
    },
    "html5_boilerplate": {
      "prefix": "ahtml5",
      "body": [
        "<!DOCTYPE html>",
        "<html lang=\"${1:en}\">",
        "<head>",
        "\t<meta charset=\"UTF-8\">",
        "\t<meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">",
        "\t<meta http-equiv=\"X-UA-Compatible\" content=\"ie=edge\">",
        "\t<title>${2:Document}</title>",
        "\t<link rel=\"stylesheet\" href=\"${3:styles.css}\">",
        "</head>",
        "<body>",
        "\t${0}",
        "</body>",
        "</html>"
      ],
      "description": "Complete HTML5 boilerplate with responsive meta tags",
      "scope": "text.html"
    },
  
    "django_block": {
      "prefix": "adblock",
      "body": [
        "{% block ${1:content} %}",
        "\t${0}",
        "{% endblock %}"
      ],
      "description": "Django template block",
      "scope": "text.html.django"
    },
    "django_static": {
      "prefix": "adstatic",
      "body": "{% static '${1:path/to/file}' %}",
      "description": "Django static file reference",
      "scope": "text.html.django"
    },
    "django_forloop": {
      "prefix": "adfor",
      "body": [
        "{% for ${1:item} in ${2:list} %}",
        "\t${0}",
        "{% endfor %}"
      ],
      "description": "Django for loop template tag",
      "scope": "text.html.django"
    },
  
    "responsive_card": {
      "prefix": "acard",
      "body": [
        "<div class=\"card\">",
        "\t<img src=\"${1:image.jpg}\" alt=\"${2:alt-text}\" class=\"card-img\">",
        "\t<div class=\"card-body\">",
        "\t\t<h3 class=\"card-title\">${3:Title}</h3>",
        "\t\t<p class=\"card-text\">${4:Description}</p>",
        "\t\t<a href=\"${5:#}\" class=\"btn btn-primary\">${6:Button}</a>",
        "\t</div>",
        "</div>"
      ],
      "description": "Bootstrap-style responsive card component",
      "scope": "text.html"
    },
    "navbar": {
      "prefix": "anavbar",
      "body": [
        "<nav class=\"navbar navbar-expand-lg navbar-light bg-light\">",
        "\t<div class=\"container-fluid\">",
        "\t\t<a class=\"navbar-brand\" href=\"#\">${1:Logo}</a>",
        "\t\t<button class=\"navbar-toggler\" type=\"button\" data-bs-toggle=\"collapse\" data-bs-target=\"#navbarNav\">",
        "\t\t\t<span class=\"navbar-toggler-icon\"></span>",
        "\t\t</button>",
        "\t\t<div class=\"collapse navbar-collapse\" id=\"navbarNav\">",
        "\t\t\t<ul class=\"navbar-nav\">",
        "\t\t\t\t<li class=\"nav-item\">",
        "\t\t\t\t\t<a class=\"nav-link active\" href=\"#\">${2:Home}</a>",
        "\t\t\t\t</li>",
        "\t\t\t\t<li class=\"nav-item\">",
        "\t\t\t\t\t<a class=\"nav-link\" href=\"#\">${3:About}</a>",
        "\t\t\t\t</li>",
        "\t\t\t</ul>",
        "\t\t</div>",
        "\t</div>",
        "</nav>"
      ],
      "description": "Responsive Bootstrap 5 navbar",
      "scope": "text.html"
    },
    "django_form": {
      "prefix": "adform",
      "body": [
        "<form method=\"${1:post}\" action=\"${2:#}\">",
        "\t{% csrf_token %}",
        "\t{{ form.as_p }}",
        "\t<button type=\"submit\" class=\"btn btn-primary\">${3:Submit}</button>",
        "</form>"
      ],
      "description": "Django form with CSRF protection",
      "scope": "text.html.django"
    },
    "input_group": {
      "prefix": "ainputgroup",
      "body": [
        "<div class=\"mb-3\">",
        "\t<label for=\"${1:id}\" class=\"form-label\">${2:Label}</label>",
        "\t<input type=\"${3:text}\" class=\"form-control\" id=\"${1:id}\" name=\"${4:name}\" placeholder=\"${5:Placeholder}\">",
        "</div>"
      ],
      "description": "Bootstrap 5 input group with label",
      "scope": "text.html"
    },
  
    "responsive_image": {
      "prefix": "aimg_responsive",
      "body": "<img src=\"${1:image.jpg}\" alt=\"${2:alt-text}\" class=\"img-fluid\" loading=\"lazy\">",
      "description": "Responsive image with lazy loading",
      "scope": "text.html"
    },
    "video_embed": {
      "prefix": "avideo_embed",
      "body": [
        "<div class=\"ratio ratio-16x9\">",
        "\t<iframe src=\"${1:https://www.youtube.com/embed/...}\" allowfullscreen></iframe>",
        "</div>"
      ],
      "description": "Responsive video embed (Bootstrap 5)",
      "scope": "text.html"
    },
  
    "skip_link": {
      "prefix": "askip_link",
      "body": "<a href=\"#main\" class=\"visually-hidden-focusable\">Skip to main content</a>",
      "description": "Accessibility skip link",
      "scope": "text.html"
    },
    "aria_alert": {
      "prefix": "aaria_alert",
      "body": "<div role=\"alert\" aria-live=\"polite\">${0}</div>",
      "description": "Accessible alert notification",
      "scope": "text.html"
    },
  
    "font_awesome": {
      "prefix": "afa",
      "body": "<i class=\"${1:fas} fa-${2:icon}\"></i>",
      "description": "Font Awesome icon",
      "scope": "text.html"
    },
    "svg_icon": {
      "prefix": "asvg",
      "body": [
        "<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"${1:16}\" height=\"${1:16}\" fill=\"currentColor\" class=\"bi bi-${2:icon}\" viewBox=\"0 0 16 16\">",
        "\t<path d=\"${3}\"/>",
        "</svg>"
      ],
      "description": "Bootstrap Icons SVG snippet",
      "scope": "text.html"
    },
  
    "container": {
      "prefix": "acontainer",
      "body": [
        "<div class=\"container\">",
        "\t${0}",
        "</div>"
      ],
      "description": "Bootstrap container",
      "scope": "text.html"
    },
    "grid_row": {
        "prefix": "aarow",
        "body": [
        "<div class=\"row\">",
        "\t<div class=\"col\">${1}</div>",
        "\t<div class=\"col\">${2}</div>",
        "</div>"
        ],
        "description": "Bootstrap grid row with 2 columns",
        "scope": "text.html"
    },

    "django_if": {
        "prefix": "aaif",
        "body": [
            "{% if ${1:condition} %}",
            "\t${2}",
            "{% else %}",
            "\t${3}",
            "{% endif %}"
        ],
        "description": "Django if-else template tag",
        "scope": "text.html.django"
    },

    "django_include": {
        "prefix": "aainclude",
        "body": "{% include '${1:template_name.html}' %}",
        "description": "Django include template tag",
        "scope": "text.html.django"
    },

    "django_url": {
        "prefix": "aaurl",
        "body": "{% url '${1:url_name}' %}",
        "description": "Django URL template tag",
        "scope": "text.html.django"
    },

    "django_extends": {
        "prefix": "aaextends",
        "body": "{% extends '${1:base.html}' %}",
        "description": "Django extends template tag",
        "scope": "text.html.django"
    },

    "bootstrap_modal": {
        "prefix": "aamodal",
        "body": [
            "<div class=\"modal fade\" id=\"${1:modalId}\" tabindex=\"-1\" aria-labelledby=\"${2:modalLabel}\" aria-hidden=\"true\">",
            "\t<div class=\"modal-dialog\">",
            "\t\t<div class=\"modal-content\">",
            "\t\t\t<div class=\"modal-header\">",
            "\t\t\t\t<h5 class=\"modal-title\" id=\"${2:modalLabel}\">${3:Modal Title}</h5>",
            "\t\t\t\t<button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>",
            "\t\t\t</div>",
            "\t\t\t<div class=\"modal-body\">",
            "\t\t\t\t${4:Modal content goes here}",
            "\t\t\t</div>",
            "\t\t\t<div class=\"modal-footer\">",
            "\t\t\t\t<button type=\"button\" class=\"btn btn-secondary\" data-bs-dismiss=\"modal\">Close</button>",
            "\t\t\t\t<button type=\"button\" class=\"btn btn-primary\">${5:Save changes}</button>",
            "\t\t\t</div>",
            "\t\t</div>",
            "\t</div>",
            "</div>"
        ],
        "description": "Bootstrap 5 modal component",
        "scope": "text.html"
    },

    "django_crispy_form": {
        "prefix": "aacrispy",
        "body": [
            "{% load crispy_forms_tags %}",
            "<form method=\"${1:post}\" action=\"${2:#}\">",
            "\t{% csrf_token %}",
            "\t{{ form|crispy }}",
            "\t<button type=\"submit\" class=\"btn btn-primary\">${3:Submit}</button>",
            "</form>"
        ],
        "description": "Django form with crispy-forms",
        "scope": "text.html.django"
    },

    "bootstrap_alert": {
        "prefix": "aaalert",
        "body": [
            "<div class=\"alert alert-${1:primary} alert-dismissible fade show\" role=\"alert\">",
            "\t${2:Alert message}",
            "\t<button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"alert\" aria-label=\"Close\"></button>",
            "</div>"
        ],
        "description": "Bootstrap 5 dismissible alert",
        "scope": "text.html"
    },

    "django_messages": {
        "prefix": "aamessages",
        "body": [
            "{% if messages %}",
            "<div class=\"messages\">",
            "\t{% for message in messages %}",
            "\t<div class=\"alert alert-{{ message.tags }} alert-dismissible fade show\" role=\"alert\">",
            "\t\t{{ message }}",
            "\t\t<button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"alert\" aria-label=\"Close\"></button>",
            "\t</div>",
            "\t{% endfor %}",
            "</div>",
            "{% endif %}"
        ],
        "description": "Django messages framework with Bootstrap alerts",
        "scope": "text.html.django"
    },

    "bootstrap_tabs": {
        "prefix": "aatabs",
        "body": [
            "<ul class=\"nav nav-tabs\" id=\"${1:myTab}\" role=\"tablist\">",
            "\t<li class=\"nav-item\" role=\"presentation\">",
            "\t\t<button class=\"nav-link active\" id=\"${2:home}-tab\" data-bs-toggle=\"tab\" data-bs-target=\"#${2:home}\" type=\"button\" role=\"tab\" aria-controls=\"${2:home}\" aria-selected=\"true\">${3:Home}</button>",
            "\t</li>",
            "\t<li class=\"nav-item\" role=\"presentation\">",
            "\t\t<button class=\"nav-link\" id=\"${4:profile}-tab\" data-bs-toggle=\"tab\" data-bs-target=\"#${4:profile}\" type=\"button\" role=\"tab\" aria-controls=\"${4:profile}\" aria-selected=\"false\">${5:Profile}</button>",
            "\t</li>",
            "</ul>",
            "<div class=\"tab-content\" id=\"${1:myTab}Content\">",
            "\t<div class=\"tab-pane fade show active\" id=\"${2:home}\" role=\"tabpanel\" aria-labelledby=\"${2:home}-tab\">${6:Home content}</div>",
            "\t<div class=\"tab-pane fade\" id=\"${4:profile}\" role=\"tabpanel\" aria-labelledby=\"${4:profile}-tab\">${7:Profile content}</div>",
            "</div>"
        ],
        "description": "Bootstrap 5 tabs component",
        "scope": "text.html"
    }

  }